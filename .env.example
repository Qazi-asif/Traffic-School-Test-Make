APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
PHP_CLI_SERVER_WORKERS=4
BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# ============================================
# QUIZ SYSTEM CONFIGURATION
# ============================================

# Disable legacy questions table (set to true to only use chapter_questions table)
DISABLE_LEGACY_QUESTIONS_TABLE=false

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=schoolplatform
DB_USERNAME=root
DB_PASSWORD=

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Queue Configuration
QUEUE_CONNECTION=database
# For production, use Redis:
# QUEUE_CONNECTION=redis

# Cache Configuration
CACHE_STORE=file
BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local

# Redis Configuration (optional)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS="noreply@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Payment Gateway Configuration
STRIPE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET=sk_test_your_stripe_secret_key_here

PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
PAYPAL_MODE=sandbox

# Authorize.Net Configuration
AUTHORIZENET_LOGIN_ID=your_authorizenet_login_id_here
AUTHORIZENET_TRANSACTION_KEY=your_authorizenet_transaction_key_here
AUTHORIZENET_MODE=sandbox

# JWT Authentication
JWT_SECRET=

# Florida State Integration - DICDS SOAP Service
FLORIDA_ENABLED=true
FLORIDA_MODE=live
FLORIDA_DICDS_WSDL=https://services.flhsmv.gov/DriverSchoolWebService/DriverSchoolWebService.asmx?WSDL
FLORIDA_DICDS_USERNAME=
FLORIDA_DICDS_PASSWORD=
FLORIDA_FALLBACK_ENABLED=true
FLORIDA_SIMULATE_SUCCESS=true

# Florida State Integration - REST API for Transmissions
FLORIDA_API_URL=https://api.flhsmv.gov/dicds/transmissions
FLORIDA_API_KEY=
FLORIDA_USERNAME=
FLORIDA_PASSWORD=
FLORIDA_SCHOOL_ID=
FLORIDA_API_TIMEOUT=30

# California TVCC Configuration
CALIFORNIA_TVCC_ENABLED=false
CALIFORNIA_TVCC_MODE=live
CALIFORNIA_TVCC_URL=https://www.dmv.ca.gov/tvcc/tvccservice
CALIFORNIA_TVCC_WSDL_URL=https://www.dmv.ca.gov/tvcc/tvccservice?wsdl
CALIFORNIA_TVCC_USER=Support@dummiestrafficschool.com
CALIFORNIA_TVCC_PASSWORD=Traffic24
CALIFORNIA_TVCC_TIMEOUT=30
CALIFORNIA_TVCC_ENVIRONMENT=production
CALIFORNIA_TVCC_FALLBACK_ENABLED=true
CALIFORNIA_TVCC_SIMULATE_SUCCESS=true

# California CTSI Configuration
CALIFORNIA_CTSI_ENABLED=false

# Nevada NTSA Configuration
NEVADA_NTSA_ENABLED=false
NEVADA_NTSA_MODE=live
NEVADA_NTSA_URL=https://secure.ntsa.us/cgi-bin/register.cgi
NEVADA_NTSA_SCHOOL_NAME="DUMMIES TRAFFIC SCHOOL.COM"
NEVADA_NTSA_TEST_NAME="DUMMIES TRAFFIC SCHOOL.COM - CA"
NEVADA_NTSA_FALLBACK_ENABLED=true
NEVADA_NTSA_SIMULATE_SUCCESS=true

# CCS Configuration
CCS_ENABLED=false
CCS_MODE=live
CCS_URL=http://testingprovider.com/ccs/register.jsp
CCS_SCHOOL_NAME=dummiests
CCS_RESULT_URL=${APP_URL}/api/ccs/result
CCS_FALLBACK_ENABLED=true
CCS_SIMULATE_SUCCESS=true

# State Transmission Execution Mode (true = synchronous, false = queued)
STATE_TRANSMISSION_SYNC=true

# State API Development Settings
STATE_API_LOG_REQUESTS=false
STATE_API_LOG_RESPONSES=false
STATE_API_SIMULATE_DELAYS=false
STATE_API_FORCE_FALLBACK=false

# AWS Configuration (optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# HTTP SOAP Configuration (for shared hosting without SOAP extension)
HTTP_SOAP_ENABLED=true
HTTP_SOAP_TIMEOUT=30
HTTP_SOAP_VERIFY_SSL=false
HTTP_SOAP_FALLBACK_MODE=soap
FLORIDA_HTTP_SOAP_ENABLED=true
CALIFORNIA_TVCC_HTTP_SOAP_ENABLED=true